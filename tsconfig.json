{
    "compilerOptions": {
        "target": "es2020",
        "module": "commonjs",
        "lib": ["es2020"],
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "strict": false,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "exclude": [
        "node_modules",
        "**/*.min.js"
    ]
}
